<script type="text/javascript">
function hideStuff(id,div1, btn) {

    document.getElementById(id).style.display = 'none';
    document.getElementById("loss1").style.display = 'none';
    document.getElementById("Description_Of_Loss").value = '';
    document.getElementById("Description_Of_Loss1").value = '';
    document.getElementById("Description_Of_Loss2").value = '';
    document.getElementById("Amount_Of_Loss").value = '';
    document.getElementById("Amount_Of_Loss1").value = '';
    document.getElementById("Amount_Of_Loss2").value = '';

    // hide the link
    btn.style.display = 'none';
    document.getElementById(div1).style.display = 'block';
}

function showStuff(id,div2, btn) {
    document.getElementById(id).style.display = 'block';
    document.getElementById("loss1").style.display = 'block';
    // hide the link
    btn.style.display = 'none';
    document.getElementById(div2).style.display = 'block';
}


function limit_of_Insurance(selBox) {
    
    if(selBox.value != '$100,000')
    {
        alert("More coverage options contact our office.");
        document.getElementById('limit_of_Insurance_at_Other_Location').value = '$100,000';
        document.getElementById('limit_of_Insurance_While_in_Transit').value = '$100,000';
    }
}

function FillBilling(f) {
  if(f.billingtoo.checked == true) {
    f.Address1.value = f.Studio_Address.value;
    f.Address1.readOnly = true;
    f.Address2.value = f.Studio_Address2.value;
    f.Address2.readOnly = true;
    f.City.value = f.Studio_City.value;
    f.City.readOnly = true;
    f.State.value = f.us_states.value;
    f.State.disabled = true;
    f.Zip.value = f.Zip_Code.value;
    f.Zip.readOnly = true;
  }
  if(f.billingtoo.checked == false) {
    f.Address1.value = '';
    f.Address1.readOnly = false;
    f.Address2.value = '';
    f.Address2.readOnly = false;
    f.City.value = '';
    f.City.readOnly = false;
    f.State.value = '';
    f.State.disabled = false;
    f.Zip.value = '';
    f.Zip.readOnly = false;
  }
}

</script>




