  <%= javascript_include_tag "bootstrap.js","rhinoslider-1.05.min.js","mousewheel.js","easing.js" %> 


    <script type="text/javascript">
        $(document).ready(function() {
                $('#slider').rhinoslider({
                    controlsPlayPause: false,
                    controlsKeyboard:false,
                    showControls: 'always',
                    showBullets: 'always',
                    controlsMousewheel: false,
                    prevText: 'Back',
                    nextText:'Proceed',
                    slidePrevDirection: 'toRight',
                    slideNextDirection: 'toLeft'
                });

                

                $(".rhino-prev").hide();
                $('.rhino-next').after('<a class="form-submit" href="javascript:void(0);" >Proceed</a>');
                $(".rhino-next").hide();




                var info = ["Step 1 ","Step 2","Step 3","Step 4","Step 5"];
                
                $('.rhino-bullet').each(function(index){
                    $(this).html(info[index]+'</a>');
                });





            });

            
            $("form").submit(function( event ) {
            
               
                $('.form-error').html("");

                var current_tab = $('#slider').find('.rhino-active').attr("id");

                switch(current_tab){
                    case 'rhino-item0':
                        step1_validation();
                        event.preventDefault();
                        break;
                    case 'rhino-item1':
                        step2_validation();
                        event.preventDefault();
                        break;
                    case 'rhino-item2':
                        step3_validation();
                        event.preventDefault();
                        break;
                    case 'rhino-item3':
                        step4_validation();
                        event.preventDefault();
                        break; 
                    case 'rhino-item4':
                        step5_validation();
                        
                        break;           
                }

                

            });

            

            $(document).on('click', '.form-submit', function(){
               
                $('.form-error').html("");

                var current_tab = $('#slider').find('.rhino-active').attr("id");

                switch(current_tab){
                    case 'rhino-item0':
                        step1_validation();
                        break;
                    case 'rhino-item1':
                        step2_validation();
                        break;
                    case 'rhino-item2':
                        step3_validation();
                        break;
                    case 'rhino-item3':
                        step4_validation();
                        break; 
                    case 'rhino-item4':
                        step5_validation();
                        break;           
                }
            });

            var step1_validation = function(){

                var err = 0;
                
                var userinput1 = $('#Email_address').val();
                var userinput2 = $('#Retype_EMail').val();
                var userinput3 = $('#Phone_Number').val();
                var userinput4 = $('#Fax').val();
                var zip_code1 = $('#Zip').val();
                var zip_code2 = $('#Zip_Code').val();

                var pattern = /^\b[A-Z0-9._%-]+@[A-Z0-9.-]+\.[A-Z]{2,4}\b$/i;
                var pattern1 = /^[(]{0,1}[0-9]{3}[)\.\- ]{0,1}[0-9]{3}[\.\- ]{0,1}[0-9]{4}$/;
                var zip_reg = /^[0-9]{5}?$/;
                
                

                if($('#start_date').val() == ''){
                    $('#start_date').parent().parent().find('.form-error').html("Please enter applicable date.");
                    err++;
                } 

                if($('#first_name').val() == ''){
                    $('#first_name').parent().parent().find('.form-error').html("Please enter first name.");
                    err++;
                }

                if($('#last_name').val() == ''){
                    $('#last_name').parent().parent().find('.form-error').html("Please enter last name.");
                    err++;
                }
                if($('#Address1').val() == ''){
                    $('#Address1').parent().parent().find('.form-error').html("Please enter address.");
                    err++;
                }

                if($('#City').val() == ''){
                      $('#City').parent().parent().find('.form-error').html("Please enter city.");
                      err++;
                }
                
                if($('#State').val() == ''){
                    $('#State').parent().parent().find('.form-error').html("This program excludes flood and windstorms in your area.");
                    err++;
                } 
                
                if(!zip_reg.test(zip_code1))
                {
                    $('#Zip').parent().parent().find('.form-error').html("Please enter correct zip code.");
                    err++;
                }

                                    

                if($('#Studio_Address').val() == ''){
                    $('#Studio_Address').parent().parent().find('.form-error').html("Please enter studio address.");
                    err++;
                }

                if($('#Studio_City').val() == ''){
                    $('#Studio_City').parent().parent().find('.form-error').html("Please enter city.");
                    err++;
                }
               
                if($('#us_states').val() == ''){
                    $('#us_states').parent().parent().find('.form-error').html("This program excludes flood, earthquake, mold and windstorms in your area.");
                    err++;
                } 

                if(!zip_reg.test(zip_code2))
                {
                    $('#Zip_Code').parent().parent().find('.form-error').html("Please enter correct zip code.");
                    err++;
                }
                
               

                if(!pattern1.test(userinput3))
                    {
                       $('#Phone_Number').parent().parent().find('.form-error').html("Please enter valid phone number.");
                    err++;
                    }


                if($('#Fax').val() != '')
                {
                    if(!pattern1.test(userinput4))
                        {
                           $('#Fax').parent().parent().find('.form-error').html("Please enter valid Fax number.");
                        err++;
                        }
                     
                }

                 if($('#Email_address').val() == ''){
                        
                        err++;
                    }

                    if(!pattern.test(userinput1))
                    {
                        $('#Email_address').parent().parent().find('.form-error').html("Please enter valid email address");  
                        err++;
                    }

                   if($('#Retype_EMail').val() == ''){
                        
                        err++;
                    }

                   if(!pattern.test(userinput2))
                    {
                       $('#Retype_EMail').parent().parent().find('.form-error').html("Please enter valid email address");
                       err++;
                    } 

                    if($('#Retype_EMail').val() != userinput1){
                        $('#Retype_EMail').parent().parent().find('.form-error').html("Please enter Re-type Email same as Email Address");
                        err++;
                   } 
                   
                    
                 

                if(err == 0){
                    $(".rhino-active-bullet").removeClass("step-error").addClass("step-success");
                    $(".rhino-next").show();
                    $('.form-submit').hide();
                    $('.rhino-next').trigger('click');
                }else{
                    $(".rhino-active-bullet").removeClass("step-success").addClass("step-error");
                }


            };

            var step2_validation = function(){
                var err = 0;
              
                if($('#higher_limits').val() == null || $('#higher_limits').val() == ""){
                    $('#higher_limits').parent().parent().find('.form-error').html("Please enter valid phone number.");
                    err++;
                }
                
                if($('#limit_of_Insurance_at_Other_Location').val() == ''){
                   $('#limit_of_Insurance_at_Other_Location').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }
                
                 if($('#limit_of_Insurance_While_in_Transit').val() == ''){
                      $('#limit_of_Insurance_While_in_Transit').parent().parent().find('.form-error').html("This is required field.");
                      err++;
                  }
                
                if($('#Policy_Deductable').val() == ''){
                    $('#Policy_Deductable').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }
                    
             

                if((!$("#paintings").is(':checked')) && (!$("#furniture").is(':checked')) && (!$("#drawings").is(':checked')) && (!$("#fabrics").is(':checked')) && (!$("#photographs").is(':checked')) && (!$("#jewelry").is(':checked')) && (!$("#sculptures").is(':checked')) && (!$("#silver").is(':checked')) && (!$("#non_Fragile").is(':checked')) && (!$("#breakable").is(':checked')) && (!$("#other").is(':checked'))){
                    
                    $('#other').parent().parent().find('.form-error').html("Please select atleast one checkbox.");
                    err++;
                }


                                
                if(err == 0){
                    $(".rhino-active-bullet").removeClass("step-error").addClass("step-success");
                    $(".rhino-next").show();
                    $('.form-submit').hide();
                    $('.rhino-next').trigger('click');
                }else{
                    $(".rhino-active-bullet").removeClass("step-success").addClass("step-error");
                }

            };

            var step3_validation = function(){
                var err = 0;

              

                if($('input[id=Studio]:checked').length<=0)
                {
                  $('#Studio').parent().parent().find('.form-error').html("This is required field.");
                  err++;
                }
                
                if($('input[id=rsales]:checked').length<=0){
                    $('#rsales').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }
                
                if($('input[id=stored_on_skits]:checked').length<=0){
                    $('#stored_on_skits').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }
                
                if($('input[id=automatic_sprinkler]:checked').length<=0){
                    $('#automatic_sprinkler').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }

                if((!$("#deadbolts").is(':checked')) && (!$("#alarm").is(':checked')) && (!$("#local_alaram").is(':checked')) && (!$("#cameras").is(':checked')) && (!$("#gates").is(':checked')) && (!$("#watchmen").is(':checked')) && (!$("#no_security").is(':checked'))){
                    
                    $('#no_security').parent().parent().find('.form-error').html("Please select atleast one checkbox.");
                    err++;
                }
                
                
                if(err == 0){
                    $(".rhino-active-bullet").removeClass("step-error").addClass("step-success");
                    $(".rhino-next").show();
                    $('.form-submit').hide();
                    $('.rhino-next').trigger('click');
                }else{
                    $(".rhino-active-bullet").removeClass("step-success").addClass("step-error");
                }

            };

            var step4_validation = function(){
                var err = 0;

                if($('#finished_art_work').val() == ''){
                    $('#finished_art_work').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }
                if($('#single_item').val() == ''){
                    $('#single_item').parent().parent().find('.form-error').html("This is required field.");
                    err++;
                }
                
                 if($('input[id=detailed_itemized]:checked').length<=0){
                      $('#detailed_itemized').parent().parent().find('.form-error').html("This is required field.");
                      err++;
                  }
                  
                   if($('input[id=record_sales]:checked').length<=0){
                        $('#record_sales').parent().parent().find('.form-error').html("This is required field.");
                        err++;
                    }
                    
                     if($('input[id=inventory_off]:checked').length<=0){
                          $('#inventory_off').parent().parent().find('.form-error').html("This is required field.");
                          err++;
                      }
                
                if(err == 0){
                    $(".rhino-active-bullet").removeClass("step-error").addClass("step-success");
                    $(".rhino-next").show();
                    $('.form-submit').hide();
                    $('.rhino-next').trigger('click');
                }else{
                    $(".rhino-active-bullet").removeClass("step-success").addClass("step-error");
                }

            };
            
            var step5_validation = function(){
                var err = 0;

                if($('#exhibits_year').val() == ''){
                   
                    err++;
                }
                if($('#current_year').val() == ''){
                    
                    err++;
                }
                
                if($('#prior_year').val() == ''){
                    
                    err++;
                } 
                
                if($('#Name_of_current_insurance_carrier').val() == ''){
                    
                    err++;
                }
                
                if(err == 0){
                    $(".rhino-active-bullet").removeClass("step-error").addClass("step-success");
                    $(".rhino-next").show();
                    $('.form-submit').hide();
                    $('.rhino-next').trigger('click');
                }else{
                    $(".rhino-active-bullet").removeClass("step-success").addClass("step-error");
                }

            };
           

        </script>
        
        
